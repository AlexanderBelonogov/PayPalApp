.popup-2
  .padding-top
  .padding-left
  .block-form-popup
    .icon-close
    %form#popup_form_2.form-popup{method: 'post'}
      %input#form_autolike_plan{name: 'plan', type: 'hidden', value: '0'}
      .block-input-form-popup.validation-block
        .null-block.info
          %img.icon-info.important{src: 'images/icon-(17).svg'}
        %input.bg-input-form-popup.one.reset{minlength: '3', name: 'url', placeholder: 'Instagram Username', required: 'required'}
      .block-input-form-popup.validation-block
        .null-block.info
          %img.icon-info.important{src: 'images/icon-(17).svg'}
        %input#count-of-photo.bg-input-form-popup.one.validation.reset{max: '99', min: '1', name: 'count', placeholder: 'Number of new photo (for example 5)', required: 'required', type: 'number'}
      .block-input-form-popup.mail
        .null-block.info
          %img.icon-info.important{src: 'images/icon-(17).svg'}
        %input.bg-input-form-popup.reset{name: 'email', placeholder: 'Email', required: 'required', type: 'email'}
      %input#popup_name_card.bg-input-form-popup{name: 'popup_name_card', type: 'hidden'}
      %input#popup_link_pay.bg-input-form-popup{name: 'popup_link_pay', type: 'hidden'}
      %input#popup_send_2{name: 'popup_submit', type: 'submit', value: 'Pay 2.90 USD'}
      %p.notification
        You buy
        %span#form_autolike_count_likes 1000
        likes on each of the
        %span#form_autolike_count_photos 0
        following photos that you will add to your profile

