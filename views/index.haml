          %img.icon-info#add-icon.add-field{src: 'images/add-(2).svg'}
          %img.icon-info.important{src: 'images/icon-(17).svg'}
        %input.bg-input-form-popup.reset{placeholder: 'Link to photo or video', minlength: '3', name: 'link_inst[]', type: 'text'}
      .block-input-form-popup.mail
        .null-block.info
          %img.icon-info.important{src: 'images/icon-(17).svg'}
        %input#popup_mail.bg-input-form-popup.reset{placeholder: 'Email', name: 'popup_mail', type: 'text'}
      %input#popup_name_card.bg-input-form-popup{name: 'popup_name_card', type: 'hidden'}
      %input#popup_link_pay.bg-input-form-popup{name: 'popup_link_pay', type: 'hidden'}
      %input#popup_send{name: 'popup_submit', type: 'submit'}

